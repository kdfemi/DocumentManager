<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UserPageController">
   <children>
      <Label fx:id="lblUsername" alignment="CENTER" contentDisplay="CENTER" layoutX="18.0" layoutY="14.0" prefHeight="50.0" prefWidth="764.0" text="Label" textAlignment="CENTER" textFill="#ed8e5f">
         <font>
            <Font name="Californian FB" size="32.0" />
         </font></Label>
      <Button fx:id="logout" layoutX="297.0" layoutY="542.0" mnemonicParsing="false" onAction="#signOut" prefHeight="32.0" prefWidth="206.0" text="Logout" />
      <Button fx:id="create" layoutX="61.0" layoutY="190.0" mnemonicParsing="false" onAction="#onCreate" text="Create" />
      <Button fx:id="upload" layoutX="61.0" layoutY="229.0" mnemonicParsing="false" onAction="#onUpload" text="Upload" />
      <Button fx:id="delete" layoutX="63.0" layoutY="264.0" mnemonicParsing="false" onAction="#onDelete" text="Delete" />
      <TableView fx:id="table" layoutX="174.0" layoutY="137.0" prefHeight="303.0" prefWidth="440.0">
        <columns>
          <TableColumn fx:id="fileName" prefWidth="306.0" text="Document name" />
          <TableColumn fx:id="status" minWidth="0.0" prefWidth="133.0" text="Uploaded" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
